<apex:page >
    <apex:includeScript value="{!$Resource.html2canvas}"/>
    <apex:includeScript value="{!$Resource.jquerymin}"/>
    <script type="text/javascript">
        $(document).ready(function() {
        var target = $('#target');
        html2canvas(target, {
            onrendered: function(canvas) {
            var data = canvas.toDataURL();
            alert(data);
            document.getElementById('imgId').src = data;
            // data is the Base64-encoded image
            }
        });
        });
    </script>
    <div id="target">
        <table cellpadding="10">
            <tr>
                <td colspan="2">
                    This is sample implementation
                </td>
            </tr>
        </table>
    </div>
    <input type="submit" value="Take Screenshot Of Div" onclick="capture();" />
    <form method="POST" enctype="multipart/form-data" id="myForm">
        <input type="textarea" name="img_val" id="img_val" value="" />
    </form>
    <div id="output">
        <img src='data:image/png;base64,' id="imgId"/>
    </div>
</apex:page>